@article{renRobustGICPBased3D2019,
  title = {Robust {{GICP-Based 3D LiDAR SLAM}} for {{Underground Mining Environment}}},
  author = {Ren, Zhuli and Wang, Liguan and Bi, Lin},
  date = {2019-01},
  journaltitle = {Sensors},
